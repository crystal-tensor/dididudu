{
  "name": "dididudu",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "backend": "uvicorn backend.app.main:app --host 0.0.0.0 --port 8001",
    "frontend": "http-server ./frontend -p 5173 --cors",
    "dev": "concurrently -n backend,frontend -c green,blue \"npm run backend\" \"npm run frontend\"",
    "start": "npm run dev"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "http-server": "^14.1.1"
  }
}